import java.util.Scanner;

public class Main {
	
	

	public static void main(String[] args) {
		

		
		Scanner sc = new Scanner ( System.in);
		
		
		
		int A , B, C, D, dif ;
		
		A = sc.nextInt();
				
		B = sc.nextInt();
		
		C = sc.nextInt();
		
		D = sc.nextInt();
		
		
		dif = A * B - C * D;
		
		
		System.out.println( "DIFERENÇA =" +  dif);
		
		
		System .out.println( " Fzer um proma para ler quatro valores inteiros A, B ,C, e D  A seguir, calcule e mostre a diferença do produto de A e B pelo proto de C e D  seguindo formula: A* B- C* D");
		
		
		sc.close();		
		
		
	}

}
